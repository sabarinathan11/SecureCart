// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Invoice {
  id          String   @id @default(cuid())
  userEmail   String
  amountCents Int
  gstCents    Int
  licenseKey  String
  pdfFileName String
  createdAt   DateTime @default(now())
  
  @@map("invoices")
}